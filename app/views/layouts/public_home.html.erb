<!DOCTYPE html>
<html>
<head>
  <title>PlotDiary</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'publichome', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'publichome', 'data-turbolinks-track': 'reload' %>
</head>

<body>

  <div class="page-wrapper">

    <header class="main-header style-v1" style="    background: rgba(38,94,145);">

      <!--Header Top-->
      <div class="header-top">
        <div class="outer-container">
          <div class="row clearfix">
            <!--Top Left-->
            <div class="top-left col-md-4 col-sm-12 col-xs-12">
              <ul class="clearfix">
                <li>welcome to Home</li>
              </ul>
            </div>
            <!--Top Right-->
            <div class="top-right pull-right col-md-8 col-sm-12 col-xs-12">
              <ul class="social-nav">
                <li><a href="#"><span class="fa fa-facebook-f"></span></a></li>
                <li><a href="#"><span class="fa fa-twitter"></span></a></li>
                <li><a href="#"><span class="fa fa-linkedin"></span></a></li>
              </ul>
              <ul class="list">
                <li><span class="icon fa fa-phone"></span>(+05) 025 024 0245</li>
                <li><a href="#"><span class="icon fa fa-envelope"></span>example.12@domain.com</a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>

      <!-- Main Box -->
      <div class="main-box">
        <div class="outer-container clearfix">
          <!--Logo Box-->
          <div class="logo-box">
            <div class="logo">
                <%= link_to image_tag("logo.png", border: 0), {action: 'index', controller: 'public_home'}, {class: 'dock-item'} %>
             </div>
          </div>

          <!--Nav Outer-->
          <div class="nav-outer clearfix">
            <!-- Main Menu -->
            <nav class="main-menu">
              <div class="navbar-header">
                <!-- Toggle Button -->      
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
              </div>

              <div class="navbar-collapse collapse clearfix">
                <ul class="navigation clearfix">
                 <li><%= link_to 'Home', root_path %></li>
                 <li><%= link_to 'PLot On Sale',  sale_plots_index_path %></li>
                 <li><%= link_to 'About Us', about_index_path %></li>
                 <li><%= link_to 'Contact Us',  contact_us_index_path %></li>
               </ul>
             </div>
           </nav>
           <!-- Main Menu End-->

           <!--Options Box-->
           <div class="options-box">
            <ul class="options-nav">

              <!--Search Box-->
              <li class="search-box-outer">
                <div class="dropdown">
                </div>
              </li>
              <li class="menu-btn hidden-bar-opener"><span class="flaticon-menu-button-of-three-lines"></span></li>
            </ul>
          </div>

        </div>
        <!--Nav Outer End-->

      </div>
    </div>
    
    <!--Sticky Header-->
    <div class="sticky-header">
      <div class="auto-container">

        <div class="outer-container clearfix">
          <!--Logo Box-->
          <div class="logo-box pull-left">
            <div class="logo"><a href="index.html"><%= image_tag("logo-small.png", alt: "")%></a></div>
          </div>

          <!--Nav Outer-->
          <div class="nav-outer clearfix">
            <!-- Main Menu -->
            <nav class="main-menu">
              <div class="navbar-header">
                <!-- Toggle Button -->      
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
              </div>

              <div class="navbar-collapse collapse clearfix">
                <ul class="navigation clearfix">
                  <li><%= link_to 'Home', root_path %></li>
                 <li><%= link_to 'PLot On Sale',  sale_plots_index_path %></li>
                 <li><%= link_to 'About Us', about_index_path %></li>
                 <li><%= link_to 'Contact Us',  contact_us_index_path %></li>
                
               </ul>
             </div>
           </nav>
           <!-- Main Menu End-->

         </div>
         <!--Nav Outer End-->

       </div>

     </div>
   </div>
   <!--End Sticky Header-->

 </header>

 <%= yield %>
